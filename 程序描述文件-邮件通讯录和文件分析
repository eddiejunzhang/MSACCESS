         Left([EmpID],3)+Mid([EmpID],6,4)
         WHERE (((tblEmpList.CompressEmpID) Is Null) AND
         ((Left([EmpID],1))>"9") AND ((Mid([EmpID],2,1))>"9") AND
         ((Mid([EmpID],3,1))>"9") AND ((Mid([EmpID],4,1))="0") AND




